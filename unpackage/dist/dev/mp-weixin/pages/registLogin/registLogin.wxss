.login{
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 20rpx 60rpx;
}
.login-pic{
	position: relative;
	margin-top: 80rpx;
	margin-bottom: 80rpx;
	width: 200rpx;
	height: 200rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.login-mess{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-top: 30rpx;
	border-bottom: #d4d4d4 1px solid;
	margin: 30rpx 20rpx;
}
.login-lable{
	font-weight: 500;
	font-size: 22px;
}
.gray{
	color: #C6C6C6;
}
.login-btn{
	width: 100%;
}
.login-other{
	position: relative;
	margin-top: 20rpx;
	width: 100%;
}
.login-other-title{
	width: 100%;
	height: 100rpx;
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.login-other-line{
	width: 30%;
	height: 2px;
	background-color: #dad9de;
}
.login-other-words{
	margin: 0 20rpx;
	color: #bab9bd;
	font-size: 20px;
}
.login-other-icon{
	width: 100%;
	height: 150rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.login-wx{
	height: 100rpx;
	width: 100rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	background-image: url(data:image/png;base64,);
	background-size: cover;
}
.login-app{
	height: 80rpx;
	width: 80rpx;
	margin: 0 20rpx;
}

