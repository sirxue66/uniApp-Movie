<view class="page"><view class="login"><image class="login-pic" src="../../static/user.png" mode="scaleToFill"></image><view class="login-mess"><view class="login-lable">用户名：</view><input type="text" placeholder="请输入用户名" placeholder-class="gray" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="login-mess"><view class="login-lable">密码：</view><input type="text" password="true" placeholder="请输入密码" placeholder-class="gray" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><button class="login-btn" type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">注册/登录</button></view><view class="login-other"><view class="login-other-title"><view class="login-other-line"></view><view class="login-other-words">第三方登录</view><view class="login-other-line"></view></view><view class="login-other-icon"><button class="login-wx" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['wxLogin',['$event']]]]]}}" bindgetuserinfo="__e"></button></view></view></view>