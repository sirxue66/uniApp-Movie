.my-header{
	position: relative;
	width: 100%;
	height: 300rpx;
	background-image: url(data:image/png;base64,);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	background-repeat: no-repeat;
	background-size: cover;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.my-pic{
	height: 180rpx;
	width: 180rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	position: relative;
	margin-left: 40rpx;
}
.my-info{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	margin-left: 20rpx;
}
.my-name{
	color: #FFFFFF;
	font-size: 24px;
	font-weight: bold;
}
.my-id{
	color: #FFFFFF;
	font-size: 18px;
}
.my-set{
	position: absolute;
	top: 40rpx;
	right: 40rpx;
	height: 60rpx;
	width: 60rpx;
}

